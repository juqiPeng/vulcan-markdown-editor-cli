    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.11.0",
    "css-loader": "^5.2.6",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^4.5.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "antd": "^4.16.12",
    "codemirror": "^5.62.0",
